<tr>
	<td>
		<%=f.select(:user_id, options_for_select(User.where(activo:true).collect{|titulo| [titulo.nombre,titulo.id]},f.object.user_id),{include_blank: "Seleccionar un usuario"},{class: 'form-control'})%>
	</td>
	<td>
		<%=f.check_box :activo, label: false%>
	</td>
	<td>
		<%= f.input_field :_destroy, as: :hidden%>
		<%= link_to '#', class: "remove_record btn btn-danger  btn-circle" do%>
			<i class="fas fa-trash"></i>
		<%end%>
	</td>
</tr>
						